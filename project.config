[project]
	description = Manage haveged using Puppet
[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group SIMP Admins
	owner = group user/simp-auto
[access "refs/for/refs/*"]
	read = group SIMP Developers
	read = group SIMP Reviewers
	read = group user/simp-auto
	label-Code-Review = -2..+2 group SIMP Developers
	label-Code-Review = -1..+1 group SIMP Reviewers
	label-Verified = -1..+1 group SIMP Developers
	label-Verified = -1..+1 group SIMP Reviewers
	rebase = group SIMP Developers
	removeReviewer = group SIMP Developers
	submit = group SIMP Developers
	abandon = group SIMP Developers
	create = group user/simp-auto
	pushMerge = group user/simp-auto
	editTopicName = group SIMP Developers
	editTopicName = group SIMP Reviewers
[access "refs/heads/*"]
	read = group user/Chris Tessmer (op-ct)
	create = group user/Chris Tessmer (op-ct)
	push = +force group user/Chris Tessmer (op-ct)
	pushMerge = group user/Chris Tessmer (op-ct)
[access "refs/tags/*"]
	push = +force group user/Chris Tessmer (op-ct)
	pushTag = group user/Chris Tessmer (op-ct)
	pushSignedTag = group user/Chris Tessmer (op-ct)
